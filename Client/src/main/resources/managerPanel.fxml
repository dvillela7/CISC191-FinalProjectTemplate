<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="560.0" prefWidth="770.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="edu.sdccd.cisc191.template.FXController">
   <Rectangle fill="#4d6f65" height="560.0" layoutX="-7.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0"
